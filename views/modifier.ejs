                    <a class="nav-link" href="#">About</a>
                </li>
            </ul>
            <form class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" type="text" placeholder="Search">
                <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
            </form>
        </div>
    </nav>


    <div class="container col-4 col-md-4">
        <h1>Modifier une Personne</h1>
        <form method="post" action="/modifier/<%= id  %>">
            <div class="form-group">
                <input type="text" class="form-control" id="numero" name="numero" value="<%= result.numero %>">
            </div>
            <div class="form-group">
                <input type="text" class="form-control" id="nom" name="nom" value="<%= result.nom %>">
            </div>
            <div class="form-group">
                <input type="text" class="form-control" id="prenom" name="prenom" value="<%= result.prenom %>">
            </div>
            <div class="form-group">
                <input type="text" class="form-control" id="email" name="email" value="<%= result.email %>">
            </div>
            <div class="form-group">
                <input type="text" class="form-control" id="telephone" name="telephone" value="<%= result.telephone %>">
            </div>
            <div class="form-group">
                <input type="text" class="form-control" id="type" name="type" placeholder="Entrer Type"
                    value="<%= result.type %>">
            </div>

            <button type="submit" class="btn btn-primary">Modifier</button>
        </form>
    </div>
</body>

</html>